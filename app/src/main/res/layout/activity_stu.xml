<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".STU"
    android:background="@drawable/abcbackground3">
    <ImageView
        android:id="@+id/e19"
        android:layout_width="450dp"
        android:layout_height="160dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:textSize="40dp"
        android:background="@drawable/s"/>
    <ImageView
        android:id="@+id/e20"
        android:layout_width="450dp"
        android:layout_height="160dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:textSize="40dp"
        android:layout_below="@id/e19"
        android:background="@drawable/t"/>
    <ImageView
        android:id="@+id/e21"
        android:layout_width="450dp"
        android:layout_height="160dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:textSize="40dp"
        android:layout_below="@id/e20"
        android:background="@drawable/u"/>
    <Button
        android:id="@+id/b20"
        android:layout_width="130dp"
        android:layout_height="30dp"
        android:text="next ->"
        android:textSize="20dp"
        android:textStyle="italic"
        android:layout_below="@id/e21"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="300dp"
        android:background="#F3F781"
        android:gravity="center"
        />
    <Button
        android:id="@+id/b21"
        android:background="@drawable/menu"
        android:layout_width="140dp"
        android:layout_height="30dp"
        android:textSize="20dp"
        android:layout_marginLeft="20dp"
        android:layout_below="@id/e21"
        android:layout_marginTop="20dp"
        android:textStyle="italic"
        android:gravity="center"
        />
</RelativeLayout>