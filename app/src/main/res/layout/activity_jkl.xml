<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="16dp"
    android:paddingLeft="16dp"
    android:paddingRight="16dp"
    tools:context=".JKL"
    android:background="@drawable/abcbackground3">
    <ImageView
        android:id="@+id/e10"
        android:layout_width="450dp"
        android:layout_height="160dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:textSize="40dp"
        android:background="@drawable/j"/>
    <ImageView
        android:id="@+id/e11"
        android:layout_width="450dp"
        android:layout_height="160dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:textSize="40dp"
        android:layout_below="@id/e10"
        android:background="@drawable/k"/>
    <ImageView
        android:id="@+id/e12"
        android:layout_width="450dp"
        android:layout_height="160dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:textSize="40dp"
        android:layout_below="@id/e11"
        android:background="@drawable/l"/>
    <Button
        android:id="@+id/b14"
        android:layout_width="130dp"
        android:layout_height="30dp"
        android:text="next ->"
        android:textSize="20dp"
        android:textStyle="italic"
        android:layout_below="@id/e12"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="300dp"
        android:background="#F3F781"
        android:gravity="center"
        />
    <Button
        android:id="@+id/b15"
        android:background="@drawable/menu"
        android:layout_width="140dp"
        android:layout_height="30dp"
        android:textSize="20dp"
        android:layout_marginLeft="20dp"
        android:layout_below="@id/e12"
        android:layout_marginTop="20dp"
        android:textStyle="italic"
        android:gravity="center"

        />
</RelativeLayout>